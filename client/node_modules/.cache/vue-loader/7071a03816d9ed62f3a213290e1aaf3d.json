ceRoot":"","sourcesContent":["<template>\n  <div class=\"challenges-page\">\n    <h1>Défis</h1>\n    <p class=\"question-count\">Question {{ currentQuestionIndex + 1 }} sur {{ questions.length }}</p>\n    <h2>{{ currentQuestion.text }}</h2>\n    <ColorScale\n      :value=\"selectedValue\"\n      @input=\"handleInput\"\n      @next-question=\"nextQuestion\"\n      ref=\"colorScale\"\n    />\n    <div v-if=\"isLastQuestion\" class=\"button-container\">\n      <button class=\"reset-btn\" @click=\"resetAnswers\">Réinitialiser</button>\n      <button class=\"validate-btn\" @click=\"submitAnswers\">Valider</button>\n    </div>\n    <!-- Tableau explicatif des couleurs -->\n    <div class=\"color-legend\">\n      <h3>Légende des Couleurs</h3>\n      <table>\n        <thead>\n          <tr>\n            <th>Couleur</th>\n            <th>Fréquence</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr>\n            <td class=\"color-box red\"></td>\n            <td>Rouge : 1 à 3 fois par semaine</td>\n          </tr>\n          <tr>\n            <td class=\"color-box orange\"></td>\n            <td>Orange : 4 à 6 fois par semaine</td>\n          </tr>\n          <tr>\n            <td class=\"color-box yellow\"></td>\n            <td>Jaune : 1 fois par jour</td>\n          </tr>\n          <tr>\n            <td class=\"color-box green\"></td>\n            <td>Vert : 2 fois par jour</td>\n          </tr>\n          <tr>\n            <td class=\"color-box blue\"></td>\n            <td>Bleu : 3 fois par jour</td>\n          </tr>\n          <tr>\n            <td class=\"color-box violet\"></td>\n            <td>Violet : 4 fois et plus par jour</td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ColorScale from '@/components/ColorScale.vue';\nimport { useChallengeStore } from '@/stores/useChallengeStore';\n\nexport default {\n  name: 'ChallengesPage',\n  components: {\n    ColorScale,\n  },\n  data() {\n    return {\n      questions: [\n        { text: \"Habituellement, à quelle fréquence consommez-vous des légumes, en dehors des pommes de terre ?\" },\n        { text: \"Habituellement, à quelle fréquence consommez-vous des légumes secs (comme des lentilles, pois chiches, haricots secs) ?\" },\n        { text: \"Habituellement, à quelle fréquence consommez-vous des féculents complets (comme du pain complet, des pâtes complètes ou du riz complet) ?\" },\n      ],\n      currentQuestionIndex: 0,\n      selectedValue: 3, // Valeur initiale au centre (4ème segment)\n      answers: [],\n      timer: null,\n    };\n  },\n  computed: {\n    currentQuestion() {\n      return this.questions[this.currentQuestionIndex];\n    },\n    isLastQuestion() {\n      return this.currentQuestionIndex === this.questions.length - 1;\n    },\n  },\n  methods: {\n    handleInput(value) {\n      this.selectedValue = value;\n    },\n    nextQuestion() {\n      const challengeStore = useChallengeStore();\n\n      // Ajouter la question actuelle et la réponse à la liste des réponses\n      this.answers.push({ question: this.currentQuestion.text, answer: this.selectedValue });\n\n      // Ajouter au store\n      challengeStore.addChallenge({\n        question: this.currentQuestion.text,\n        answer: this.selectedValue,\n      });\n      \n      challengeStore.addChallengeResult({\n        question: this.currentQuestion.text,\n        answer: this.selectedValue,\n        score: this.calculateScore(this.selectedValue),\n      });\n\n      // Réinitialiser la sélection pour la prochaine question\n      this.selectedValue = 3; // Réinitialise l'aiguille au centre pour la prochaine question\n      this.$refs.colorScale.clearSelection(); // Réinitialiser la sélection de l'arc\n\n      if (this.currentQuestionIndex < this.questions.length - 1) {\n        this.currentQuestionIndex++;\n      } else {\n        alert('Merci pour vos réponses !');\n      }\n    },\n    calculateScore(selectedValue) {\n      return selectedValue > 3 ? 1 : 0; // Exemple simple pour calculer le score\n    },\n    submitAnswers() {\n      console.log('Réponses:', this.answers);\n      alert('Réponses soumises !');\n    },\n    resetAnswers() {\n      this.answers = [];\n      this.currentQuestionIndex = 0;\n      this.selectedValue = 3;\n      this.$refs.colorScale.clearSelection(); // Réinitialiser la sélection de l'arc\n    }\n  },\n};\n</script>\n\n<style scoped>\n.challenges-page {\n  text-align: center;\n  margin-top: 50px;\n}\n\nh1 {\n  font-size: 32px;\n  margin-bottom: 20px;\n}\n\n.question-count {\n  font-size: 18px;\n  margin-bottom: 10px;\n  color: #555;\n}\n\nh2 {\n  font-size: 24px;\n  margin-bottom: 20px;\n}\n\n.validate-btn,\n.reset-btn {\n  padding: 15px 30px;\n  font-size: 18px;\n  border-radius: 5px;\n  border: none;\n  cursor: pointer;\n  transition: background-color 0.3s;\n  margin: 0 10px;\n}\n\n.validate-btn {\n  background-color: #4CAF50;\n  color: white;\n}\n\n.validate-btn:hover {\n  background-color: #45a049;\n}\n\n.reset-btn {\n  background-color: #f44336;\n  color: white;\n}\n\n.reset-btn:hover {\n  background-color: #d32f2f;\n}\n\n/* Style pour le tableau explicatif des couleurs */\n.color-legend {\n  margin-top: 30px;\n  padding: 10px;\n  border-top: 2px solid #ccc;\n  display: inline-block;\n  max-width: 400px;\n  text-align: left;\n}\n\n.color-legend h3 {\n  font-size: 18px;\n  margin-bottom: 10px;\n}\n\n.color-legend table {\n  width: 100%;\n  border-collapse: collapse;\n  font-size: 14px;\n}\n\n.color-legend th,\n.color-legend td {\n  border: 1px solid #ddd;\n  padding: 5px;\n  text-align: left;\n}\n\n.color-legend th {\n  background-color: #f4f4f4;\n}\n\n.color-box {\n  width: 20px;\n  height: 20px;\n  display: inline-block;\n  border-radius: 5px;\n}\n\n.red { background-color: red; }\n.orange { background-color: orange; }\n.yellow { background-color: yellow; }\n.green { background-color: green; }\n.blue { background-color: blue; }\n.violet { background-color: violet; }\n\n/* Alignement des boutons */\n.button-container {\n  text-align: center;\n  margin-top: 20px;\n}\n\n.reset-btn, .validate-btn {\n  margin: 0 5px; /* Réduit l'espacement entre les boutons */\n}\n</style>"]}]}