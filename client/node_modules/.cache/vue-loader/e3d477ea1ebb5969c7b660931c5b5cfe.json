AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;YACpE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;iBACtC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;iBAClD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;iBACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;iBAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACxB,CAAC,CAAC,CAAC,CAAC,CAAC;QACP,CAAC,CAAC,CAAC,CAAC,CAAC;MACP,CAAC,CAAC,CAAC,CAAC,CAAC;IACP,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/home/student/Bureau/HabitsGame/client/src/views/RecipePage.vue","sourceRoot":"","sourcesContent":["<<template>\n  <div class=\"page-container\">\n    <!-- Catégories alimentaires à gauche -->\n    <div class=\"food-categories left-categories\">\n      <h2>Catégories Alimentaires</h2>\n      <div v-for=\"(food, category) in leftCategories\" :key=\"category\" class=\"food-category\">\n        <h3>{{ category }}</h3>\n        <div class=\"food-icons\">\n          <div v-for=\"item in food\" :key=\"item.name\" class=\"draggable-container\">\n            <img :src=\"require(`@/assets/${item.icon}`)\" \n                 @dragstart=\"startDrag($event, item.icon, category)\" \n                 @dragend=\"dragEnd\"\n                 class=\"food-icon\" \n                 draggable=\"true\"/>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Contenu central -->\n    <div class=\"central-content\">\n      <h1>Ma recette</h1>\n      <div class=\"plate-container\">\n        <!-- Petite assiette pour l'entrée -->\n        <div class=\"plate-container-item\">\n          <div class=\"plate-title\">Entrée</div>\n          <div class=\"plate small-plate entry-plate\" @drop=\"drop($event, 'starter')\" @dragover=\"allowDrop($event)\">\n            <div class=\"plate-content\">\n              <img v-for=\"dish in starterDish\" :key=\"dish\" :src=\"require(`@/assets/${dish}`)\" alt=\"Entrée\" class=\"food-icon\" />\n            </div>\n          </div>\n        </div>\n\n        <!-- Grande assiette pour le plat principal -->\n        <div class=\"plate-container-item\">\n          <div class=\"plate-title\">Plat Principal</div>\n          <div class=\"plate large-plate\" @drop=\"drop($event, 'main')\" @dragover=\"allowDrop($event)\">\n            <div class=\"plate-content\">\n              <img v-for=\"dish in mainDish\" :key=\"dish\" :src=\"require(`@/assets/${dish}`)\" alt=\"Plat Principal\" class=\"food-icon\" />\n            </div>\n          </div>\n        </div>\n\n        <!-- Petite assiette pour le dessert -->\n        <div class=\"plate-container-item\">\n          <div class=\"plate-title\">Dessert</div>\n          <div class=\"plate small-plate dessert-plate\" @drop=\"drop($event, 'dessert')\" @dragover=\"allowDrop($event)\">\n            <div class=\"plate-content\">\n              <img v-for=\"dish in dessertDish\" :key=\"dish\" :src=\"require(`@/assets/${dish}`)\" alt=\"Dessert\" class=\"food-icon\" />\n            </div>\n          </div>\n        </div>\n\n        <!-- Petite assiette pour la boisson -->\n        <div class=\"plate-container-item\">\n          <div class=\"plate-title\">Boisson</div>\n          <div class=\"plate small-plate drink-plate\" @drop=\"drop($event, 'drink')\" @dragover=\"allowDrop($event)\">\n            <div class=\"plate-content\">\n              <img v-for=\"drink in drinkDish\" :key=\"drink\" :src=\"require(`@/assets/${drink}`)\" alt=\"Boisson\" class=\"food-icon\" />\n            </div>\n          </div>\n        </div>\n      </div>\n      <!-- Bouton de réinitialisation -->\n      <button @click=\"resetPage\" class=\"reset-button\">Réinitialiser la recette</button>\n      <!-- Bouton de vérification de la recette -->\n      <button @click=\"checkRecipe\" class=\"check-button\">Vérifier la recette</button>\n      <!-- Message de santé -->\n      <div v-if=\"healthMessage\" class=\"health-message\">{{ healthMessage }}</div>\n    </div>\n\n    <!-- Catégories alimentaires à droite -->\n    <div class=\"food-categories right-categories\">\n      <h2>Catégories Alimentaires</h2>\n      <div v-for=\"(food, category) in rightCategories\" :key=\"category\" class=\"food-category\">\n        <h3>{{ category }}</h3>\n        <div class=\"food-icons\">\n          <div v-for=\"item in food\" :key=\"item.name\" class=\"draggable-container\">\n            <img :src=\"require(`@/assets/${item.icon}`)\" \n                 @dragstart=\"startDrag($event, item.icon, category)\" \n                 @dragend=\"dragEnd\"\n                 class=\"food-icon\" \n                 draggable=\"true\"/>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { useRecipeStore } from '@/stores/useRecipeStore';\n\nexport default {\n  name: 'RecipePage',\n  setup() {\n    const recipeStore = useRecipeStore();\n\n    const addNewRecipe = (recipe) => {\n      recipeStore.addRecipe(recipe);\n    };\n\n    const removeRecipe = (recipeId) => {\n      recipeStore.removeRecipe(recipeId);\n    };\n\n    return { recipeStore, addNewRecipe, removeRecipe };\n  },\n  data() {\n    return {\n      leftCategories: {\n        \"Légumes et Fruits\": [\n          { name: \"Carotte\", icon: \"carotte.png\" },\n          { name: \"Tomate\", icon: \"tomate.png\" }\n        ],\n        \"Céréales et Dérivés\": [\n          { name: \"Riz\", icon: \"riz.png\" },\n          { name: \"Pain\", icon: \"pain.png\" }\n        ],\n        \"Lait et Produits Laitiers\": [\n          { name: \"Lait\", icon: \"lait.png\" },\n          { name: \"Yaourt\", icon: \"yaourt.png\" }\n        ]\n      },\n      rightCategories: {\n        \"Viandes, Volailles, Poissons et Produits de la Mer, Œufs\": [\n          { name: \"Boeuf\", icon: \"boeuf.png\" },\n          { name: \"Saumon\", icon: \"saumon.png\" }\n        ],\n        \"Matières Grasses\": [\n          { name: \"Huile\", icon: \"huile.png\" },\n          { name: \"Beurre\", icon: \"beurre.png\" }\n        ],\n        \"Sucre et Produits Sucrés\": [\n          { name: \"Sucre\", icon: \"sucre.png\" },\n          { name: \"Bonbons\", icon: \"bonbons.png\" }\n        ],\n        \"Boissons\": [\n          { name: \"Eau\", icon: \"eau.png\" },\n          { name: \"Jus\", icon: \"jus.png\" }\n        ]\n      },\n      mainDish: [],\n      starterDish: [],\n      dessertDish: [],\n      drinkDish: [],\n      draggedIcon: null,\n      draggedCategory: null,\n      healthMessage: '', // Variable pour stocker le message de santé\n      healthScore: 100 // Score initial de santé\n    };\n  },\n  methods: {\n    startDrag(event, icon, category) {\n      event.dataTransfer.setData('text/plain', JSON.stringify({ icon, category }));\n      this.draggedIcon = icon;\n      this.draggedCategory = category;\n    },\n    dragEnd() {\n      this.draggedIcon = null;\n      this.draggedCategory = null;\n    },\n    allowDrop(event) {\n      event.preventDefault();\n    },\n    drop(event, dishType) {\n      event.preventDefault();\n      const { icon, category } = JSON.parse(event.dataTransfer.getData('text/plain'));\n\n      // Retirer l'élément de la catégorie d'origine\n      let categories;\n      if (category in this.leftCategories) {\n        categories = this.leftCategories;\n      } else if (category in this.rightCategories) {\n        categories = this.rightCategories;\n      }\n\n      if (categories) {\n        const index = categories[category].findIndex(item => item.icon === icon);\n        if (index !== -1) {\n          categories[category].splice(index, 1);\n        }\n      }\n\n      // Ajouter l'élément à l'assiette correspondante\n      if (dishType === 'main') {\n        this.mainDish.push(icon);\n      } else if (dishType === 'starter') {\n        this.starterDish.push(icon);\n      } else if (dishType === 'dessert') {\n        this.dessertDish.push(icon);\n      } else if (dishType === 'drink') {\n        this.drinkDish.push(icon);\n      }\n    },\n    resetPage() {\n      location.reload();  // Recharge la page entière\n    },\n    checkRecipe() {\n      this.healthScore = 100; // Réinitialisation du score à 100\n      let emptyPlates = 0; // Compteur pour les assiettes vides\n\n      // Vérification des assiettes vides\n      if (this.starterDish.length === 0) {\n        emptyPlates += 1;\n      }\n      if (this.mainDish.length === 0) {\n        emptyPlates += 1;\n      }\n      if (this.dessertDish.length === 0) {\n        emptyPlates += 1;\n      }\n      if (this.drinkDish.length === 0) {\n        emptyPlates += 1;\n      }\n\n      if (emptyPlates === 4) {\n        // Toutes les assiettes sont vides\n        this.healthMessage = \"Vos assiettes sont vides.\";\n        this.healthScore = 0;\n      } else {\n        // Diminution du score si les matières grasses ou produits sucrés sont dans les mauvais plats\n        if (this.mainDish.some(dish => this.isFat(dish)) || this.mainDish.some(dish => this.isSugar(dish))) {\n          this.healthScore -= 30;\n        }\n        if (this.starterDish.some(dish => this.isFat(dish)) || this.starterDish.some(dish => this.isSugar(dish))) {\n          this.healthScore -= 30;\n        }\n        if (this.drinkDish.some(drink => !this.isDrink(drink))) {\n          this.healthScore -= 30;\n        }\n\n        // Affichage du message de santé selon le score final\n        if (this.healthScore === 100) {\n          this.healthMessage = `Votre repas est parfaitement sain avec un score de ${this.healthScore}%.`;\n        } else if (this.healthScore > 75) {\n          this.healthMessage = `Votre repas est sain à ${this.healthScore}%.`;\n        } else if (this.healthScore > 50) {\n          this.healthMessage = `Votre repas est modérément sain avec un score de ${this.healthScore}%.`;\n        } else {\n          this.healthMessage = `Attention, votre repas est peu sain avec un score de seulement ${this.healthScore}%.`;\n        }\n      }\n    },\n    isFat(dish) {\n      // Vérifie si le plat appartient à la catégorie \"Matières Grasses\"\n      return this.rightCategories[\"Matières Grasses\"].some(item => item.icon === dish);\n    },\n    isSugar(dish) {\n      // Vérifie si le plat appartient à la catégorie \"Sucre et Produits Sucrés\"\n      return this.rightCategories[\"Sucre et Produits Sucrés\"].some(item => item.icon === dish);\n    },\n    isDrink(dish) {\n      // Vérifie si le plat appartient à la catégorie \"Boissons\"\n      return this.rightCategories[\"Boissons\"].some(item => item.icon === dish);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.page-container {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  padding: 20px;\n  height: 100vh;\n}\n\n.food-categories {\n  width: 20%;\n}\n\n.left-categories, .right-categories {\n  text-align: center;\n}\n\n.central-content {\n  width: 60%;\n  text-align: center;\n  position: relative;\n}\n\n.plate-container {\n  display: flex;\n  justify-content: center;\n  align-items: flex-end;\n  position: relative;\n  height: 400px;\n}\n\n.plate-container-item {\n  position: relative;\n  margin: 0 15px;\n}\n\n.plate-title {\n  position: absolute;\n  top: -30px; /* Ajuste la position du titre au-dessus de l'assiette */\n  left: 50%;\n  transform: translateX(-50%);\n  font-size: 16px; /* Ajuste la taille de la police */\n  font-weight: bold;\n  color: #333; /* Couleur du texte */\n}\n\n.large-plate,\n.entry-plate,\n.dessert-plate,\n.drink-plate {\n  /* Styles pour les assiettes, comme précédemment */\n}\n\n.large-plate {\n  width: 